{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint-config-airbnb",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [
      2
    ],
    "linebreak-style": 0,
    "prettier/prettier": "error"
  },
  "plugins": [
    "prettier"
  ]
}
